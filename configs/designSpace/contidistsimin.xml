<design_space>
    <simulator name="ServerSimulator" type="simulator" ><!--type="synthetic"--><!--type can be synthetic or simulator-->
        <parameter name="maximumTimeOfASimulation" value="5"/><!--in minutes--> 
		<parameter name="minimumTimeOfASimulation" value="60"/><!--in seconds-->
        <parameter name="realSimulator" value="ContinentalSimulator"/>
        <!-- If you put TEMP here than the dumps are placed into the temporary folder of the OS -->
        <parameter name="benchmark_model_directory" value="D:\fadse_conti\fadse_dumps\contibase" />    
		<parameter name="comsol_directory" value="configs\comsoldirectory.txt" />
		<parameter name="simulator_output_file" value="D:\fadse_conti\fadse_dumps\conti#" />              
    </simulator>
    <benchmarks>
        <item name="Demo_mag2_modif.m;Demo_therm2_modif.m" />        				     
    </benchmarks>
    <database ip="192.168.1.1" port="7" name="fadse_conti" user="a" password="a"/>
    <metaheuristic name="NSGAII" config_path="nsgaii.properties" />
    <parameters>
        <parameter name="w_I.txt" description="param1" type="integer" min="20" max="60" step="5" divideBy="10" />
        <parameter name="w_hcoil.txt" description="param2" type="integer" min="0" max="40" step="5" divideBy="10"/>               
		<parameter name="w_lcoil.txt" description="param2" type="integer" min="0" max="20" step="5" divideBy="10"/>               
		<parameter name="w_lmagnet.txt" description="param2" type="integer" min="0" max="40" step="5" divideBy="100"/>               
		<parameter name="w_liron_move.txt" description="param2" type="integer" min="0" max="30" step="5" divideBy="10"/>               
		<parameter name="w_liron.txt" description="param2" type="integer" min="0" max="16" step="4" divideBy="10"/>               
		<parameter name="w_hiron.txt" description="param2" type="integer" min="0" max="30" step="5" divideBy="10"/>               
		<parameter name="w_hiron2.txt" description="param2" type="integer" min="0" max="30" step="5" divideBy="10"/>               
    </parameters>
    <system_metrics>        
        <system_metric name="mean_z.dat" type="float" unit="" desired="small"/>
        <system_metric name="stddev_z.dat" type="float" unit="" desired="small" />
		<system_metric name="Bmax1.dat" type="float" unit="" desired="small" />
	 	<system_metric name="Volm1.dat" type="float" unit="" desired="small" />
		<system_metric name="F_per_mv.dat" type="float" unit="" desired="small" />
		<system_metric name="Tcoil1.dat" type="float" unit="" desired="small" />
		<system_metric name="Text1.dat" type="float" unit="" desired="small" />
		<system_metric name="Fillfact1.dat" type="float" unit="" desired="small" />
    </system_metrics>
    <rules>
    </rules>
	<relations>
	</relations>
	<output output_path="d:/conti/output" />	
</design_space>
